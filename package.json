    "typescript": ">=4.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.0.1",
    "@typescript-eslint/eslint-plugin": ">=5.42.0",
    "@typescript-eslint/parser": ">=5.42.0",
    "eslint-config-airbnb": "19.0.4",
